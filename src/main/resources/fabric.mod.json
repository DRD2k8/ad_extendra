{
  "schemaVersion": 1,
  "id": "ad_extendra",
  "version": "${version}",
  "name": "Ad Extendra",
  "description": "A mod to extend Ad Astra with new planets, rocket tiers, and more!",
  "authors": [
    "Dandi2k8 (Co-owner)"
  ],
  "contributors": [
    "Teddy0008 (Real owner)"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/ad_extendra-continuation",
    "sources": "https://github.com/DRD2k8/ad_extendra",
    "issues": "https://github.com/DRD2k8/ad_extendra/issues"
  },
  "license": "MIT",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.drd.ad_extendra.client.ModClient"
    ],
    "main": [
      "com.drd.ad_extendra.Main"
    ]
  },
  "mixins": [
    "ad_extendra.mixins.json",
    {
      "config": "ad_extendra.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "ad_astra": "=1.15.5",
    "botarium": ">=2.2.0",
    "resourcefullib": ">=2.1.11",
    "resourcefulconfig": ">=2.1.0"
  }
}
